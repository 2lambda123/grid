{
  // where is your cloudformation stack running?
  aws: {
    region: 'eu-west-1',
    profile: 'media-service',
    stackName: 'media-service-DEV'
  },

  // Domain root of your site, e.g. foobar.co.uk
  domainRoot: '',

  panda: {
    // The domain for users email addresses
    userDomain: '',

    // The bucket containing the Pan Domain Auth settings for the domain

    }
  },

  crier: {
    live: {
      streamName: '',
      roleArn: ''
    },
    preview: {
      streamName: '',
      roleArn: ''
    }
  },

  composer: {
    url: ''
  },

  google: {
    tracking: {
      id: ''
    }
  }
}
